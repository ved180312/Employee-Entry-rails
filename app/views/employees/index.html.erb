

<div> <h2>listing employee </h2></div>

<table>
  <thead>
    <tr>
      <th>name</th>
      <th>email</th>
      <th>address</th>
    </tr>
  </thead>

  <tbody>
    <% @emp.each do |emp| %>
      <tr>
        <td><%= emp.first_name %></td>
        <td><%= emp.personal_email %></td>
        <td><%= emp.pincode %>
      </tr>
    <% end %>
  </tbody>
</table>













